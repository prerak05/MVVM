<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.mvvm.module.indiacoronastatus.view.activity.StateWiseActivity"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name="com.example.mvvm.module.indiacoronastatus.view.activity.DateWiseCaseActivity"
            android:theme="@style/AppTheme.ActionBar" />
        <activity
            android:name=".module.sidedrawer.DrawerActivity"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".module.registration.view.RegistrationActivity" />
        <activity android:name=".module.login.view.LoginActivity" />
        <activity android:name=".module.splashscreen.view.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>